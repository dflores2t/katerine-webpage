n        .fa-caret-right {\n          margin-left: 10px;\n        }\n      }\n      .anterior {\n        margin-top: 20px;\n        display: flex;\n        justify-content: center;\n        align-items: center;\n        .fa-caret-left {\n          margin-right: 10px;\n        }\n      }\n      button[type=\"button\"] {\n        background-color: $ColorLogo;\n        box-shadow: none;\n        border: none;\n        font-size: 1.4rem;\n        min-width: 70%;\n      }\n      i {\n        font-size: 2rem;\n        color: $ColorMenu;\n      }\n    }\n    .card {\n      background-color: transparent;\n      margin: 5px;\n    }\n    .card-title {\n      text-align: center;\n      font-family: \"Parisienne\", cursive;\n      font-size: 3rem;\n      font-weight: 500;\n      color: $ColorMenu;\n    }\n  }\n\n  footer {\n    color: $ColorMenu;\n    .redesSociales {\n      display: flex;\n      justify-content: flex-end;\n      align-items: center;\n      list-style: none;\n      margin-right: 4rem;\n      li i {\n        color: $ColorMenu;\n        font-size: 1.5rem;\n        background: $ColorLogo;\n        margin-left: 1.3rem;\n        padding: 0.4rem;\n        border-radius: 50%;\n      }\n    }\n    .dot ul {\n      display: flex;\n      justify-content: center;\n      align-items: center;\n      list-style: none;\n      li {\n        margin-left: 1rem;\n      }\n      i {\n        font-size: 1.2rem;\n      }\n      .activo {\n        border-radius: 50%;\n        border: 2px solid $ColorLogo;\n        padding: 6px;\n        color: $ColorLogo;\n      }\n    }\n    #line {\n      border-left: 4px solid white;\n      border-top: 4px solid white;\n    }\n  }\n}\n\n// media queries\n// @media (min-width: 1012px) {\n//   header {\n//      nav .test {\n//     //   background-color: red;\n//     //  height: 300px;\n//     }\n//     nav .test1 {\n//      // background-color: blue;\n//     }\n//   }\n// }\n","scss/_menu.scss":".container-fluid {\n  header {\n    nav {\n      background-color: $ColorFondo;\n      // display: flex;\n      ul li,\n      .dropdown {\n        margin-right: 10px;\n        padding: 5px;\n      }\n      ul li:hover {\n        background-color: $ColorLogo;\n        border-top-left-radius: 20px;\n        border-bottom-right-radius: 20px;\n      }\n\n      a {\n        color: $ColorMenu;\n        font-size: 1.2rem;\n      }\n      .active {\n        position: relative;\n        padding-left: 10px;\n        padding-right: 10px;\n      }\n\n      .active::after,\n      .active::before {\n        content: \"\";\n        position: absolute;\n        border: solid 2px $ColorMenu;\n      }\n      .active::after {\n        left: 10px;\n        right: 0px;\n        top: 0px;\n        bottom: 10px;\n        border-left: none;\n        border-bottom: none;\n      }\n      .active::before {\n        top: 10px;\n        bottom: 0px;\n        left: 0px;\n        right: 10px;\n        border-top: none;\n        border-right: none;\n      }\n\n      form {\n        input[type=\"search\"] {\n          border-radius: 0px 0px 0px 0px;\n          background-color: #000;\n          border: none;\n          color: $ColorMenu;\n          border-left: 5px solid $SeparadorSearch;\n        }\n        input[type=\"search\"]:focus {\n          box-shadow: none;\n        }\n      }\n      i.fa-bars {\n        color: $ColorMenu;\n        font-size: 3rem;\n      }\n    }\n    // style Dorpdow menu\n\n    li .dropdown {\n      position: relative;\n      display: inline-block;\n    }\n    .dropdown-menu {\n      background-color: transparent;\n      min-width: 160px;\n      box-shadow: 0px 8px 16px 0px rgba(0, 0, 0, 0.2);\n    }\n    // Style for each item listo\n    .dropdown-menu li {\n      color: $ColorMenu;\n      padding: 5px 0px;\n      text-decoration: none;\n      display: block;\n      text-align: left;\n      margin-top: 7px;\n      border-bottom: 1px solid $SeparadorSearch;\n      border-bottom-right-radius: 20px;\n    }\n    .dropdown-menu li:hover {\n      background-color: $ColorLogo;\n      border-top-left-radius: 20px;\n      border-bottom-right-radius: 20px;\n    }\n    .dropdown-menu a:hover {\n      background-color: transparent;\n    }\n  }\n}\n","scss/contact.scss":"@use 'variables';\n.jumbotron {\n  margin-top: 14%;\n  background: #ff0e8d;\n  color: #fff;\n  border-radius: 0px;\n}\n.jumbotron-sm {\n  padding-top: 24px;\n  padding-bottom: 24px;\n}\n.jumbotron small {\n  color: #fff;\n}\n.h2 small {\n  font-size: 24px;\n}\n"},"lineCount":null}},"error":null,"hash":"8e4a88cba1616a2a77837196034ebff4","cacheData":{"env":{}}}